    "Google",
    "GitHub",
    "Cors",
    "Docker",
    "Cookie"
  ],
  "author": "ERODRIGUEZ-10",
  "license": "ISC",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "artillery": "^2.0.7",
    "nodemon": "^3.0.3",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "bcrypt": "^5.1.1",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
